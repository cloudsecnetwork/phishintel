{
  "name": "phishintel",
  "version": "1.0.0",
  "description": "Built by CloudSec Network (CSN)",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --detectOpenHandles --forceExit",
    "test:ci": "jest --ci --coverage --detectOpenHandles --forceExit --verbose",
    "test:all": "node scripts/test-runner.js",
    "start": "nodemon --exec babel-node app.js",
    "start:prod": "node dist/app.js",
    "build": "babel --out-dir dist",
    "lint": "eslint . --ext .js --config .eslintrc.cjs --ignore-pattern 'client/' --ignore-pattern 'node_modules/' --ignore-pattern 'coverage/'",
    "lint:fix": "eslint . --ext .js --config .eslintrc.cjs --fix --ignore-pattern 'client/' --ignore-pattern 'node_modules/' --ignore-pattern 'coverage/'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudsecnetwork/phishintel.git"
  },
  "author": "CSN",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cloudsecnetwork/phishintel/issues"
  },
  "homepage": "https://github.com/cloudsecnetwork/phishintel#readme",
  "devDependencies": {
    "@babel/cli": "^7.25.7",
    "@babel/core": "^7.25.7",
    "@babel/node": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "supertest": "^6.3.4",
    "mongodb-memory-server": "^9.1.6",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "@babel/plugin-transform-modules-commonjs": "^7.25.7"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-fileupload": "^1.5.1",
    "helmet": "^8.0.0",
    "html-validate": "^8.25.0",
    "htmlparser2": "^9.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.7.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.15"
  }
}
